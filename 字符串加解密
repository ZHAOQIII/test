def f():
    L = list('abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ0123456789')
    L2 = list('BCDEFGHIJKLMNOPQRSTUVWXYZAbcdefghijklmnopqrstuvwxyza1234567890')
    D = dict(zip(L, L2))
    D2 = dict(zip(L2, L))
    a = input()
    b = input()
    print(''.join([D[i] for i in a]))
    print(''.join([D2[i] for i in b]))
try:
    while True:
        f()
except:
    pass
